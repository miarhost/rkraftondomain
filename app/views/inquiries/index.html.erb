<p id="notice"><%= notice %></p>
<h1>Inquiries</h1>
<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Second name</th>
      <th>Email</th>
      <th>Subject</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquiries.each do |inquiry| %>
      <tr>
        <td><%= inquiry.first_name %></td>
        <td><%= inquiry.second_name %></td>
        <td><%= inquiry.email %></td>
        <td><%= inquiry.subject %></td>
        <td><%= inquiry.message %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>